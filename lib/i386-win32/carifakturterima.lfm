  object qFaktur: TZReadOnlyQuery
    Connection = frmLogin.Koneksi
    SQL.Strings = (
      'select ft.id,ft.no, s.supplier,'
      'DATE_FORMAT(ft.tempo,''%d/%m/%Y'') as tempo, '
      'DATE_FORMAT(ft.tgl,''%d-%m-%Y'') as tgl,'
      'if(ft.lunas=1,''Lunas'',''Belum'') as Lunas,'
      'if(ft.simpan=1,''Ya'',''Tidak'') as Tersimpan'
      'from faktur_terima as ft'
      'left join supplier as s on s.id = ft.supplier'
      'order by ft.tgl DESC'
    )
    Params = <>
    left = 436
    top = 20
  end
  object dFaktur: TDatasource
    DataSet = qFaktur
